{
  "name": "GaMa",
  "manifest_version": 3,
  "version": "1.0",
  "description": "anti fraud detection Extension for you online safety.",
  "permissions": ["tabs", "activeTab", "contextMenus", "notifications"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
