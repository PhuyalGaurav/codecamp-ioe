{
    "name": "GaMa",
    "manifest_version": 3,
    "version": "1.0",
    "description": "anti phising extension that works af. :)",
    "permissions": [
      "tabs", "activeTab", "contextMenus"
    ],
    "content_scripts": [
      {
        "matches": ["http://*/*"],
        "js": ["contentScript.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  
}